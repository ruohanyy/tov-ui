  },
  "dependencies": {
    "@floating-ui/vue": "^1.1.5",
    "@tov-ui-study/utils": "workspace:*",
    "@v-c/utils": "^0.0.26",
    "lodash-es": "^4.17.21"
  },
  "devDependencies": {
    "@types/lodash-es": "^4.17.12"
  }
